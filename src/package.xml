<?xml version = "1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>caret_analyze</name>
  <version>0.2.2</version>
  <description>CARET's tools for analyzing trace results</description>
  <maintainer email="hasegawa@isp.co.jp">hsgwa</maintainer>
  <license>Apache License 2.0</license>

  <depend>python3-multimethod-pip</depend>
  <exec_depend>tracetools_analysis</exec_depend>
  <exec_depend>python3-tqdm</exec_depend>
  <exec_depend>python3-yaml</exec_depend>
  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>
  <test_depend>ament_mypy</test_depend>
  <test_depend>python3-pytest-mock</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
